{
  "collectCoverageFrom": [
    "**/*.js",
    "!__tests__/coverage/**/*",
    "!__tests__/visual/dist/**/*",
    "!dist/**/*",
    "!webpack.*.js",
    "!babel.*.js",
    "!.eslintrc.js"
  ],
  "coverageDirectory": "__tests__/coverage/",
  "testRegex": "\\-test\\.js$",
  "transform": {
    "^.+\\.[t|j]sx?$": "babel-jest"
  }
}
